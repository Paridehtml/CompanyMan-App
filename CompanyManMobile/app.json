{
  "expo": {
    "name": "CompanyManMobile",
    "slug": "CompanyManMobile",
    "version": "1.0.0",
    "icon": "./assets/images/icon.png",
    "scheme": "companymanmobile",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.CompanyManMobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.CompanyManMobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "Company Man",
      "shortName": "CompanyMan",
      "description": "Restaurant Management System",
      "themeColor": "#306eff",
      "backgroundColor": "#ffffff",
      "display": "standalone", 
      "scope": "/",
      "startUrl": "/"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}